<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- ANHDIEPMMK BEGIN -->

    <style name="button_caculator">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/btn_number_selector</item>
        <item name="android:textSize">@dimen/btn_num_size</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_height">@dimen/dialog_key_height</item>
    </style>

    <!-- ANHDIEPMMK END -->


    <!-- TRIEN BEGIN -->
    <style name="product_info">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#000</item>
        <item name="android:typeface">serif</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowRadius">1.0</item>
    </style>

    <style name="style_more_app_title">
        <item name="android:textSize">19.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#fd1e00</item>
        <item name="android:typeface">serif</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">4.0</item>
    </style>

    <style name="style_tv_continues">
        <item name="android:textSize">25.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:typeface">sans</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">4.0</item>
        <item name="android:paddingLeft">@dimen/winscreen_tv_continues_padding_left_right</item>
        <item name="android:paddingRight">@dimen/winscreen_tv_continues_padding_left_right</item>
    </style>

    
     <style name="style_team_slogan">
        <item name="android:textSize">19.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff0000ff</item>
        <item name="android:typeface">serif</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">4.0</item>
    </style>
    
     <style name="button_caculator1">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/btn_number_selector</item>
        <item name="android:textSize">@dimen/btn_num_size</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_height">@dimen/dialog_key_height</item>
    </style>
    
     <style name="button_ok">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/btn_number_selector</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    
     <style name="button_del">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/btn_number_selector</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <!-- TRIEN END -->

</resources>